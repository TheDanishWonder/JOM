{
  "expo": {
    "name": "smashed",
    "description": "The social drinking game",
    "slug": "drinking-app",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "drinkingapp",
    "primaryColor": "#ffb677",
    "icon": "src/images/logo.png",
    "splash": {
      "image": "assets/splash.gif",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "drinkingapp",
      "versionCode": 1,
      "icon": "src/images/logo.png",
      "permissions": []
    }
  }
}
